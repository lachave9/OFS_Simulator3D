; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="FunscriptSimulator3D"
run/main_scene="res://Main.tscn"
config/features=PackedStringArray("4.5", "C#")
boot_splash/show_image=false
config/icon="res://favicon.png"

[display]

window/size/viewport_width=400
window/size/transparent=true
window/per_pixel_transparency/allowed=true
window/per_pixel_transparency/enabled=true

[dotnet]

project/assembly_name="FunscriptSimulator3D"

[global]

verbose=false
renderer=false

[gui]

common/drop_mouse_on_gui_input_disabled=true

[internationalization]

rendering/text_driver="ICU / HarfBuzz / Graphite (Built-in)"

[mono]

project/assembly_name="FunscriptSimulator3D"

[network]

limits/websocket_client/max_in_buffer_kb=1048576

[physics]

common/enable_pause_aware_picking=true

[rendering]

textures/vram_compression/import_etc2_astc=true
viewport/transparent_background=true
environment/defaults/default_environment="res://default_env.tres"
quality/filters/msaa=2
